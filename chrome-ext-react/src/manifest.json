{
  "manifest_version": 3,
  "name": "Profit Peak",
  "description": "A Web3 Extension",
  "version": "0.1",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.svg"
  },
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "unlimitedStorage"
],
  "host_permissions": [
      "https://*/*",
      "http://*/*"
  ]
}
